    "chalk": "3.0.0",
    "clone": "^2.1.2",
    "css-loader": "3.4.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.5.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "^4.5.0",
